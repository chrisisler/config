snippet sopl
	System.out.println($1);
snippet sop
	System.out.print($1);
snippet soplv
	System.out.println("$1 is: " + $1);
snippet trycatch
	try
	{
		$1
	}
	catch
	{
		$2
	}
snippet fn
	${1:public} ${2:static }${3:void} ${4:funcName}($5)
	{
		$6
	}
snippet new
	${1:type} ${2:name} = new $1($3);
snippet class
	${1:public} class ${2:name}
	{
		$3
	}
